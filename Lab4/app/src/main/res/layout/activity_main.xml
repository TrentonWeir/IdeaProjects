<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button8"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="8"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="426dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="3"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="542dp" />

    <Button
        android:id="@+id/button0"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="0"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="-/+"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="."
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/buttonEq"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="="
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/buttonPlus"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="+"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="542dp" />

    <Button
        android:id="@+id/buttonX"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="X"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="426dp" />

    <Button
        android:id="@+id/buttonDivid"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="/"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="368dp" />

    <Button
        android:id="@+id/buttonSpace"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="_"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="368dp" />

    <Button
        android:id="@+id/buttonPEnd"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text=")"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="368dp" />

    <Button
        android:id="@+id/buttonPStart"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="("
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="368dp" />

    <Button
        android:id="@+id/button22"
        android:layout_width="77dp"
        android:layout_height="58dp"
        android:text="DEL"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="310dp" />

    <Button
        android:id="@+id/button23"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="C"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="310dp" />

    <Button
        android:id="@+id/buttonMin"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="-"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="484dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="1"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="542dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="4"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="484dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="6"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="484dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="2"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="542dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="5"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="484dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="7"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="426dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="68dp"
        android:layout_height="58dp"
        android:text="9"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="426dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="350dp"
        android:layout_height="72dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="6dp"
        android:text="1 + 2"
        app:layout_constraintBottom_toTopOf="@+id/button22"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="sans-serif"
        android:text="Result:"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/editTextNumber"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>